<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hc.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".score.ScoreActivity"
            android:label="Double Image" />
        <activity
            android:name=".box.BoxActivity"
            android:label="Box" />
        <activity
            android:name=".round.RoundActivity"
            android:label="Ball" />
        <activity
            android:name=".fixed.FixedActivity"
            android:label="Fixed" />
        <activity android:name=".slide.SlideActivity" />
        <activity android:name=".hot.HotWordActivity" />
        <activity android:name=".circle.CircleActivity" />
        <activity android:name=".percent.PercentActivity" />
        <activity android:name=".percent.RingPercentActivity"></activity>
    </application>

</manifest>